<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:controls="clr-namespace:ReInvented.Animations.Controls"
    xmlns:vm="clr-namespace:ReInvented.Animations.ViewModels">

    <DataTemplate DataType="{x:Type vm:RecentItemsViewModel}">
        <controls:RecentItemsControl />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:NewProjectViewModel}">
        <controls:NewProjectControl />
    </DataTemplate>

</ResourceDictionary>