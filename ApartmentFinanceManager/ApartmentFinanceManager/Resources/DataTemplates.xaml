<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:views="clr-namespace:ApartmentFinanceManager.Views"
    xmlns:vm="clr-namespace:ApartmentFinanceManager.ViewModels">

    <DataTemplate DataType="{x:Type vm:SummaryViewModel}">
        <views:SummaryView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:PaymentViewModel}">
        <views:PaymentView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:ExpenseViewModel}">
        <views:ExpenseView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:CommonExpenseViewModel}">
        <views:CommonExpenseView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:ReportViewModel}">
        <views:ReportView />
    </DataTemplate>

</ResourceDictionary>