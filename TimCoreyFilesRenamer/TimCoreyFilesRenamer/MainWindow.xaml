<Window
    x:Class="TimCoreyFilesRenamer.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:TimCoreyFilesRenamer"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:vm="clr-namespace:TimCoreyFilesRenamer.ViewModels"
    Title="MainWindow"
    Width="800"
    Height="450"
    mc:Ignorable="d">
    <Window.DataContext>
        <vm:MainViewModel />
    </Window.DataContext>
    <Grid>
        <Button
            Width="150"
            Height="28"
            HorizontalAlignment="Center"
            VerticalAlignment="Center"
            Click="OnRenameFilesClicked"
            Content="Rename Files" />
    </Grid>
</Window>
